library(glmmTMB)
d <- prec.sf %>% mutate(pos = numFactor(prec.sf$X, prec.sf$Y), group = factor(rep(1, nrow(prec.sf))))
mod.TMB <- glmmTMB(Jul ~ elev + exp(pos + 0 | group), data=d)
View(d)
summary(mod.TMB)
drop1(mod.TMB, test="Chisq")
r2(mod.TMB)
